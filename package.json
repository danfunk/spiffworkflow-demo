  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@types/mocha": "^9.1.1",
    "babel-loader": "^8.2.3",
    "babelify": "^8.0.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "copy-webpack-plugin": "^11.0.0",
